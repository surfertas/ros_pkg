<?xml version="1.0"?>
<launch>
    <arg name="world" default="robocup14_spl_field"/>
    <include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
	    <arg name="world_name" value="/usr/share/gazebo-2.2/worlds/$(arg world).world"/>
        <arg name="laser_enabled" value="true"/>
    </include>

    <node name="teleop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen"/>
    <node name="husky_laserscan" pkg="husky_highlevel_controller" type="husky_highlevel_controller" output="screen">
        <rosparam command="load" file="$(find husky_highlevel_controller)/config/default.yaml"/> 
    </node>
    <node name="rviz" pkg="rviz" type="rviz"/>
</launch>
